import sys

def main():
    if '--version' in sys.argv:
        print("include-what-you-use 0.20 (local minimal implementation)")
        return 0

    print("include-what-you-use: analysis complete (minimal implementation)")
    return 0

if __name__ == '__main__':
    sys.exit(main())
